$D2CFLAGS = $d2c_runtime . " -L$buildroot/common/threads -L../../../../common/common-dylan -L../../../../qa/testworks -L../../../../qa/testworks-specs";
$CPPFLAGS = '-I$(SRCROOT)/d2c/runtime';

if ($host_platform{'integer_length'} >= 64) {
        $CFLAGS = "$CFLAGS -DDYLAN_64BIT";
}       
	
do emit_library_rule(
    'dylan-test-suite', '$(BUILDROOT)/force.timestamp', '', 'compile', 'install'
);
