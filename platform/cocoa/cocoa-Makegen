# common makegen stuff for the "cocoa" libraries under d2c.
# each makegen should &makegen_include("../cocoa-Makegen"), and also
# add any of the other common libraries they use to D2CFLAGS.

$D2CFLAGS = $d2c_runtime . " -L$buildroot/common/common-dylan -L$buildroot/common/threads ";
$CPPFLAGS = '-I$(SRCROOT)/d2c/runtime -I/System/Library/Frameworks/AppKit.framework/Headers';

